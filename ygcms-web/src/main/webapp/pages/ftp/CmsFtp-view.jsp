<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<form  class="form-horizontal">
	           <div class="form-group">
                    <label class="col-lg-3 control-label">名称:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="ftpnameID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">服务器地址:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="ipID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">端口:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="portID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">登陆名:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="usernameID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">密码:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="passwordID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">编码:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="encodingID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">超时时间:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="timeoutID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">目录:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="ftppathID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">访问地址:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="urlID"></p>
                    </div>
                </div>
</form>